      <div class="col-sm-6 px-0 mt-3">
        <div class="card">
  <div class="card-header">
    案件一覧
  </div>
  <div class="list-group">
  <% for (var i in project) { %>
  <a href="/projectDetail/<%- i %>" class="border-top-0 border-right-0 border-left-0 list-group-item list-group-item-action">
